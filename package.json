{
    "name": "react-paypal-express-checkout",
    "version": "1.0.5",
    "description": "React component that renders Paypal's express check out button",
    "repository": {
        "type": "git",
        "url": "https://github.com/thinhvo0108/react-paypal-express-checkout.git"
    },
